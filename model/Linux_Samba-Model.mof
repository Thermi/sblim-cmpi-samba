// ===================================================================
// Linux_SambaShareProtocolOptions
// ===================================================================
[IBM_HasInstances, IBM_HasProvider, Description]
class Linux_SambaShareProtocolOptions : CIM_SettingData {

    [Write, ValueMap ("\"Auto\",\"winnt\",\"win2k\""), Values (
        "\"0\",\"1\",\"2\""), IBM_ImplementationType ("Manual")
        , Description (
        "This parameter specifies what OS ACL semantics should be "
        "compatible with. Possible values are winnt for Windows NT 4,"
        " win2k for Windows 2000 and above and auto. If you specify "
        "auto, the value for this parameter will be based upon the "
        "version of the client. There should be no reason to change "
        "this parameter from the default.\n\n")]
    uint8 AclCompatibility;

    [Write, IBM_ImplementationType ("Manual"), Description (
        "This boolean parameter controls whether smbd(8) will allow "
        "clients to attempt to store OS/2 style Extended attributes "
        "on a share. In order to enable this parameter the underlying"
        " filesystem exported by the share must support extended "
        "attributes (such as provided on XFS and EXT3 on Linux, with "
        "the correct kernel patches). On Linux the filesystem must "
        "have been mounted with the mount option user_xattr in order "
        "for extended attributes to work, also extended attributes "
        "must be compiled into the Linux kernel.")]
    boolean EASupport;

    [Write, IBM_ImplementationType ("Manual"), Description (
        "This boolean parameter controls whether smbd(8) will attempt"
        " to map UNIX permissions into Windows NT access control "
        "lists. This parameter was formally a global parameter in "
        "releases prior to 2.2.2.\n\n")]
    boolean NTACLSupport;

    [Key, Write, IBM_ImplementationType ("Manual")]
    string Name;
};


